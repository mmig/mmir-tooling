{
  "name": "mmir-tooling",
  "version": "6.2.0-beta.1",
  "description": "Build tools for MMIR projects",
  "directories": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/mmig/mmir-tooling/"
  },
  "keywords": [
    "mmir",
    "cordova"
  ],
  "bin": {
    "mmirinstall": "./bin/cli-mmir-install.js"
  },
  "scripts": {
    "build": "npm run build:clean && tsc && npm run build:copy && npm run build:assets",
    "build:clean": "node -e require('fs-extra').emptyDirSync('build')",
    "build:copy": "node -e require('fs-extra').copy('build','./')",
    "build:assets": "node -e require('fs-extra').copy('assets','./')",
    "test": "node test.js",
    "doc": "npm run doc:clean && npm run doc:gen",
    "doc:clean": "node -e require('fs-extra').emptyDirSync('docs')",
    "doc:gen": "cross-var typedoc --name \"$npm_package_name $npm_package_version\" index.d.ts"
  },
  "author": "MMIG team",
  "license": "MIT",
  "dependencies": {
    "@scion-scxml/scxml": "^4.3.25",
    "array-flatten": "^3.0.0",
    "es6-promise": "^4.2.8",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "meow": "^7.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.161",
    "@types/meow": "^5.0.0",
    "@types/node": "^14.11.2",
    "cross-var": "^1.1.0",
    "eclint": "^2.8.1",
    "mmir-lib": "^6.1.0",
    "typedoc": "^0.19.2",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typedoc-plugin-markdown": "^3.0.3",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "mmir-lib": "^6.2.0"
  }
}
