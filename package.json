{
  "name": "mmir-tooling",
  "version": "6.1.0",
  "description": "Build tools for MMIR projects",
  "directories": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/mmig/mmir-tooling/"
  },
  "keywords": [
    "mmir",
    "cordova"
  ],
  "bin": {
    "mmirinstall": "./bin/cli-mmir-install.js"
  },
  "scripts": {
    "test": "node test.js",
    "doc": "npm run clean-doc && npm run gen-doc",
    "clean-doc": "node -e require('fs-extra').emptyDirSync('docs')",
    "gen-doc": "cross-var typedoc --name \"$npm_package_name $npm_package_version\" index.d.ts"
  },
  "author": "MMIG team",
  "license": "MIT",
  "dependencies": {
    "@scion-scxml/scxml": "^4.3.25",
    "array-flatten": "^3.0.0",
    "es6-promise": "^4.2.8",
    "fs-extra": "^8.0.0",
    "lodash": "^4.17.15",
    "meow": "^6.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.149",
    "@types/meow": "^5.0.0",
    "cross-var": "^1.1.0",
    "eclint": "^2.8.1",
    "mmir-lib": "^6.1.0",
    "typedoc": "^0.16.11",
    "typedoc-plugin-external-module-name": "^3.0.0",
    "typedoc-plugin-markdown": "^2.2.17"
  },
  "peerDependencies": {
    "mmir-lib": "^6.1.0"
  }
}
